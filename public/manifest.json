{
  "id": "net.anweisen.watched",
  "short_name": "Watched",
  "name": "Watched: your Time Wasted",
  "description": "Analyze how much time you have spent watching tv series in your life",
  "author": "anweisen",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "64x64 32x32 24x24 16x16",
      "type": "image/x-icon"
    },
    {
      "src": "/android-chrome-192x192.png",
      "sizes": "192x192",
      "type": "image/png"
    },
    {
      "src": "/android-chrome-512x512.png",
      "sizes": "512x512",
      "type": "image/png"
    }
  ],
  "start_url": ".",
  "display": "standalone",
  "display_override": [
    "window-controls-overlay",
    "standalone",
    "browser"
  ],
  "orientation": "portrait",
  "theme_color": "#23232A",
  "background_color": "#23232A",
  "handle_links": "preferred",
  "client_mode": ["navigate-existing", "auto"],
  "scope_extensions": [
    {"origin": "watched.anweisen.net"},
    {"origin": "watched-beta.anweisen.net"}
  ],
  "dir": "ltr"
}
